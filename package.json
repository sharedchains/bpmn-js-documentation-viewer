{
  "name": "bpmn-js-documentation-viewer",
  "version": "0.0.1",
  "description": "A simple module extension for bpmn-js to show documentation content",
  "main": "index.js",
  "scripts": {
    "all": "run-s lint test bundle",
    "bundle": "webpack",
    "bundle:watch": "webpack -w",
    "start": "webpack serve --content-base ./example --open-page index.html",
    "lint": "eslint .",
    "test": "karma start",
    "dev": "npm test -- --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.sh4.red/luca.bonora/bpmn-js-documentation-viewer.git"
  },
  "keywords": [
    "bpmn-js",
    "bpmn-js-module",
    "documentation",
    "viewer",
    "html"
  ],
  "author": "Luca Bonora <luca.bonora@sharedchains.com>",
  "license": "MIT",
  "devDependencies": {
    "bpmn-js": "^8.3.0",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^8.1.1",
    "eslint": "^7.24.0",
    "eslint-plugin-bpmn-io": "^0.12.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.3.2",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^8.0.0",
    "raw-loader": "^4.0.2",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "bootstrap": "^4.6.1",
    "camunda-modeler-plugin-helpers": "^3.4.0-alpha.1",
    "jquery": "^3.6.0",
    "min-dash": "^3.5.2",
    "min-dom": "^3.1.3",
    "popper.js": "^1.16.1",
    "react-dom": "^17.0.2",
    "svg.js": "^2.6.3"
  }
}
