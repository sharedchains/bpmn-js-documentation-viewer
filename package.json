{
  "name": "bpmn-js-documentation-viewer",
  "version": "0.0.1",
  "description": "A simple module extension for bpmn-js to show documentation content",
  "main": "index.js",
  "scripts": {
    "all": "run-s lint test bundle",
    "bundle": "webpack --mode production",
    "bundle:watch": "webpack -w",
    "start": "webpack serve --content-base ./example --open",
    "lint": "eslint .",
    "test": "karma start",
    "dev": "npm test -- --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.sh4.red/luca.bonora/bpmn-js-documentation-viewer.git"
  },
  "keywords": [
    "bpmn-js",
    "bpmn-js-module",
    "documentation",
    "viewer",
    "html"
  ],
  "author": "Luca Bonora <luca.bonora@sharedchains.com>",
  "license": "MIT",
  "devDependencies": {
    "bpmn-js": "^8.6.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^8.1.1",
    "eslint": "^7.26.0",
    "eslint-plugin-bpmn-io": "^0.12.0",
    "file-drops": "^0.4.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.4.0",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^9.1.1",
    "raw-loader": "^0.5.1",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "min-dash": "^3.5.2",
    "min-dom": "^3.1.3",
    "svg.js": "^2.6.3"
  }
}
